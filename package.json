{
  "name": "mobile-almendros",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --clear",
    "start:dev": "EXPO_PUBLIC_ENV=development expo start --clear",
    "start:dev-fresh": "EXPO_PUBLIC_DEV_FORCE_LOGIN=true expo start --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "android:dev": "EXPO_PUBLIC_ENV=development expo start --android",
    "android:dev-fresh": "EXPO_PUBLIC_DEV_FORCE_LOGIN=true expo start --android",
    "ios": "expo start --ios",
    "ios:dev": "EXPO_PUBLIC_ENV=development expo start --ios",
    "ios:dev-fresh": "EXPO_PUBLIC_DEV_FORCE_LOGIN=true expo start --ios",
    "web": "expo start --web",
    "web:dev-fresh": "EXPO_PUBLIC_DEV_FORCE_LOGIN=true expo start --web",
    "lint": "eslint .",
    "cache:clear": "expo start --clear",
    "prebuild": "npx expo prebuild --clean",
    "postinstall": "patch-package",
    "test": "jest --verbose --no-cache",
    "test:watch": "jest --watch --verbose --no-cache",
    "test:coverage": "jest --coverage --verbose --no-cache",
    "test:clear": "jest --clearCache && npm run test",
    "test:debug": "jest --verbose --no-cache --detectOpenHandles --runInBand",
    "test:ci": "jest --ci --coverage --watchAll=false --verbose --no-cache --runInBand",
    "test:auth": "jest --testPathPattern=authService --verbose --no-cache",
    "test:auth-only": "jest src/features/auth/api/__tests__/authService.test.ts --verbose --no-cache",
    "test:purchase": "jest --testNamePattern=\"PurchaseCard\" --verbose --no-cache",
    "test:basic": "jest src/__test__/basic.test.ts --verbose --no-cache",
    "test:formatters": "jest src/shared/utils/__tests__/formatters.test.ts --verbose --no-cache",
    "test:components": "jest src/shared/components/__tests__ --verbose --no-cache",
    "test:single": "jest --testPathPattern",
    "test:updateSnapshot": "jest --updateSnapshot",
    "test:silent": "jest --silent",
    "test:bail": "jest --bail --verbose",
    "test:verify": "bash test-verification.sh",
    "cache:clear:windows": "if exist node_modules\\.cache rmdir /s /q node_modules\\.cache",
    "cache:clear:full": "npm run cache:clear:windows && jest --clearCache && if exist .expo rmdir /s /q .expo",
    "test:clean": "npm run cache:clear:full && npm test"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/stack": "^7.3.2",
    "@shopify/flash-list": "1.7.6",
    "ajv": "^6.12.6",
    "axios": "^1.9.0",
    "expo": "^53.0.9",
    "expo-blur": "~14.1.4",
    "expo-constants": "^17.1.6",
    "expo-font": "~13.3.0",
    "expo-haptics": "~14.1.4",
    "expo-image": "^2.1.7",
    "expo-linking": "~7.1.4",
    "expo-router": "^5.0.6",
    "expo-secure-store": "^14.2.3",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "jest-expo": "~53.0.5",
    "moment": "^2.30.1",
    "nanoid": "^3.3.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-paper": "^5.14.1",
    "react-native-pdf": "^6.7.7",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-share": "^12.0.11",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.72.8",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "jest": "^29.7.0",
    "jest-expo": "^53.0.1",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "resolutions": {
    "nanoid": "3.3.4",
    "**/nanoid": "3.3.4",
    "@react-navigation/core/nanoid": "3.3.4",
    "@react-navigation/native/nanoid": "3.3.4",
    "react-test-renderer": "19.0.0"
  },
  "overrides": {
    "react-test-renderer": "19.0.0"
  }
}